<body>
    @RenderSection("ScriptsTop", required: false)
    <!-- header -->
	<div class="header">
        @await Html.PartialAsync("_HeaderPartial")
    </div>
    @*@await Html.PartialAsync("_LoginPopupPartial")*@
    <!-- nav -->
   <div class="movies_nav">
       @await Html.PartialAsync("_MovieNarPartial")
   </div>
    <!-- //nav -->
    @RenderBody()
    <!-- footer -->
    <div class="footer">
        @await Html.PartialAsync("_FooterPartial")
    </div>
    <!-- //footer -->
    
    <!-- //js -->
    <!-- banner-bottom-plugin -->
    <script defer src="~/movie/js/owl.carousel.js" asp-append-version="true"></script>
    <script defer  asp-append-version="true">
        $(document).ready(function () {
            $("#owl-demo").owlCarousel({

                autoPlay: 3000, //Set AutoPlay to 3 seconds

                items: 5,
                itemsDesktop: [640, 4],
                itemsDesktopSmall: [414, 3]

            });

        });
    </script>
    <!-- start-smoth-scrolling -->
    <script defer type="text/javascript" src="~/movie/js/move-top.js" asp-append-version="true"></script>
    <script defer type="text/javascript" src="~/movie/js/easing.js" asp-append-version="true"></script>
    <script defer type="text/javascript" asp-append-version="true">
            $('.toggle').click(function(){
                // Switches the Icon
                $(this).children('i').toggleClass('fa-pencil');
            // Switches the forms
            $('.form').animate({
                height: "toggle",
			'padding-top': 'toggle',
			'padding-bottom': 'toggle',
			opacity: "toggle"
		  }, "slow");
		});
        jQuery(document).ready(function ($) {
            $(".scroll").click(function (event) {
                event.preventDefault();
                $('html,body').animate({ scrollTop: $(this.hash).offset().top }, 1000);
            });
        });
    </script>
   
    <!-- start-smoth-scrolling -->

    <!-- Bootstrap Core JavaScript -->
    <script defer src="~/movie/js/bootstrap.min.js" asp-append-version="true"></script>
    <script defer asp-append-version="true">
        $(document).ready(function () {
            $(".dropdown").hover(
                function () {
                    $('.dropdown-menu', this).stop(true, true).slideDown("fast");
                    $(this).toggleClass('open');
                },
                function () {
                    $('.dropdown-menu', this).stop(true, true).slideUp("fast");
                    $(this).toggleClass('open');
                }
            );
        });
    </script>
    <!-- //Bootstrap Core JavaScript -->
    <!-- here stars scrolling icon -->
    <script defer type="text/javascript" asp-append-version="true">
        $(document).ready(function () {
			/*
				var defaults = {
				containerID: 'toTop', // fading element id
				containerHoverID: 'toTopHover', // fading element hover id
				scrollSpeed: 1200,
				easingType: 'linear'
				};
			*/

            $().UItoTop({ easingType: 'easeOutQuart' });

        });
    </script>
    <!-- //here ends scrolling icon -->
    <div id="fb-root"></div>
    <script defer asp-append-version="true">
        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s); js.id = id;
            js.src = "//connect.facebook.net/vi_VN/sdk.js#xfbml=1&version=v2.10&appId=846411685524427";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
    </script>

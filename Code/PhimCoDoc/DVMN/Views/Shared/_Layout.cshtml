@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet
<!DOCTYPE html>
<html lang="vi">
<head>
    @{
        string scheme = Context.Request.Scheme;
        string host = Context.Request.Host.Host;
        string port = Context.Request.Host.Port.ToString();
        string slug = Context.Request.Path;
        string url = string.Format("{0}://{1}:{2}{3}", scheme, host, port, slug);
    }
    <title>@ViewData["Title"]</title>
    <meta name="description" content="@ViewData["Desciption"]" />
    <meta name="keywords" content="@ViewData["Keyword"]" />
